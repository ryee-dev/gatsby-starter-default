{
  "name": "gatsby-admin-dash",
  "version": "1.0.0",
  "private": true,
  "author": "Roger Yee <roger@humnutrition.com>",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "commit": "npx git-cz",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.29",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-sharp": "^2.2.28",
    "gatsby-plugin-styled-components": "^3.1.8",
    "gatsby-plugin-typescript": "^2.1.11",
    "gatsby-remark-copy-linked-files": "^2.1.24",
    "gatsby-remark-responsive-iframe": "^2.2.21",
    "gatsby-remark-smartypants": "^2.1.11",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-transformer-remark": "^2.6.27",
    "gatsby-transformer-sharp": "^2.2.20",
    "modern-normalize": "^0.6.0",
    "normalize.css": "^8.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.14",
    "styled-components": "^5.0.0-beta.9",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.9.0",
    "@types/config": "^0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^6.1.1",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/express": "^4.17.1",
    "@types/js-cookie": "^2.2.2",
    "@types/node": "^13.5.1",
    "@types/prettier": "^1.18.2",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-helmet": "^5.0.11",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.1.19",
    "@types/stylelint": "^9.10.1",
    "@types/superagent": "^4.1.3",
    "@types/yup": "^0.26.23",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-blvd": "^1.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-unicorn": "^0.5.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-unicorn": "15.0.1",
    "prettier": "^1.18.2",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
