{
  "name": "gatsby-ts",
  "version": "1.0.0",
  "private": true,
  "author": "Roger Yee <roger@humnutrition.com>",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "commit": "npx git-cz",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@types/config": "^0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^6.1.8",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.3",
    "@types/js-cookie": "^2.2.5",
    "@types/node": "^14.0.5",
    "@types/prettier": "^2.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/rebass": "^4.0.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^5.0.1",
    "@types/stylelint": "^9.10.1",
    "@types/superagent": "^4.1.7",
    "@types/theme-ui": "^0.3.1",
    "@types/yup": "^0.29.0",
    "babel-plugin-styled-components": "^1.10.6",
    "framer-motion": "^2.0.0-beta.72",
    "gatsby": "^2.22.12",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-copy-linked-files": "^2.3.2",
    "gatsby-remark-responsive-iframe": "^2.4.2",
    "gatsby-remark-smartypants": "^2.3.1",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-remark": "^2.8.13",
    "gatsby-transformer-sharp": "^2.5.2",
    "modern-normalize": "^0.6.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.0.0",
    "react-hot-loader": "^4.12.21",
    "rebass": "^4.0.7",
    "styled-components": "^5.1.1",
    "theme-ui": "^0.3.1",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-blvd": "^1.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-unicorn": "^0.5.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-plugin-unicorn": "20.1.0",
    "prettier": "^2.0.5",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
